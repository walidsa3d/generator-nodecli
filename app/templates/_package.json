{
  "name": "<%= moduleName %>",
  "version": "1.0.0",
  "description": "<%= dequote(moduleDesc) %>",
  "main": "lib.js",
  "scripts": {
    "test": "mocha",
    "changelog": "rm CHANGELOG.md && changelog https://github.com/<%= githubName %>/<%= moduleName %> all > CHANGELOG.md && git add . && git commit . -m changelog && git push"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/<%= githubName %>/<%= moduleName %>"
  },
  "bin": {
      "<%= cliName %>" : "bin/cli"
     },
  "keywords": <%= JSON.stringify(keywords) %> ,
  "author": "<%= dequote(author) %>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/<%= githubName %>/<%= moduleName %>/issues"
  },
  "dependencies": {
    "commander": "^2.9.0",
    "prettyjson": "^1.1.3"
  },
  "devDependencies": {
    "changelog": "^1.0.7",
    "chai": "~1.10.0",
    "mocha": "^3.1.0"
  }
}
